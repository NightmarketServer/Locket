[mitm]
hostname = api.revenuecat.com

[script]
# 📤 Script sửa Header để tránh cache, bỏ ETag
script-request-header Locket-Header ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$) https://raw.githubusercontent.com/NightmarketServer/Locket/refs/heads/main/Locket-header.js

# 📥 Script giả lập phản hồi Gold Premium
script-response-body Locket-Gold ^https:\/\/api\.revenuecat\.com\/.+\/(receipts$|subscribers\/[^/]+$) https://raw.githubusercontent.com/NightmarketServer/Locket/refs/heads/main/Locket.js
